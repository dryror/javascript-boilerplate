{
  "name": "javascript-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for JavaScript web apps",
  "keywords": ["JavaScript", "es6", "react", "webpack"],
  "author": "Rory Drysdale <rorydrysdale@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dryror/javascript-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/dryror/javascript-boilerplate/issues"
  },
  "homepage": "https://github.com/dryror/javascript-boilerplate#readme",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src",
    "clean": "rm -rf build",
    "bundle": "webpack -p --config webpack.config.prod.js",
    "build": "npm run lint && npm run clean && npm run bundle",
    "deploy": "npm run build && gh-pages -d build",
    "start": "webpack-dev-server --inline --hot"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "~2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.10.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-css-modules": "^3.7.5",
    "react-dom": "^0.14.7"
  }
}
