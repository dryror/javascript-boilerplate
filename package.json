{
  "name": "javascript-boilerplate",
  "version": "1.0.0-beta.1",
  "description": "Boilerplate for JavaScript web apps",
  "main": "src/index.js",
  "dependencies": {
    "react": "15.0.0",
    "react-dom": "15.0.0",
    "react-redux": "4.4.1",
    "redux": "3.3.1"
  },
  "devDependencies": {
    "babel-core": "6.7.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.7.2",
    "css-loader": "0.23.1",
    "css-modules-require-hook": "4.0.0",
    "enzyme": "2.2.0",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "4.3.0",
    "expect": "1.16.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "gh-pages": "0.11.0",
    "html-webpack-plugin": "2.15.0",
    "jsdom": "8.3.0",
    "mocha": "2.4.5",
    "react-addons-test-utils": "15.0.0",
    "redbox-react": "1.2.3",
    "style-loader": "0.13.1",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "scripts": {
    "build": "npm run lint && npm run clean && npm run bundle",
    "bundle": "webpack -p --config webpack.config.prod.js",
    "clean": "rm -rf build",
    "deploy": "npm run build && gh-pages -d build",
    "lint": "eslint src/**/*.js",
    "start": "node server.js",
    "test": "mocha test.config.js src/**/*.spec.js",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dryror/javascript-boilerplate.git"
  },
  "keywords": [
    "JavaScript",
    "boilerplate"
  ],
  "author": "Rory Drysdale <rorydrysdale@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dryror/javascript-boilerplate/issues"
  },
  "homepage": "https://github.com/dryror/javascript-boilerplate#readme"
}
